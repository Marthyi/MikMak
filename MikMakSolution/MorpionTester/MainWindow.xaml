<Window x:Class="MorpionTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="200" />
            <RowDefinition Height="28" />
            <RowDefinition Height="28" />
            <RowDefinition Height="83*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="200" />
            <ColumnDefinition Width="303*" />
        </Grid.ColumnDefinitions>
        <TextBox Grid.Column="2" Grid.Row="0" Name="textBox1"  />
        <Button Content="GetState" Grid.Column="1" Grid.Row="1"  Name="button1" Click="button1_Click" />
        <TextBox Grid.Column="2" Grid.Row="1" Name="textBoxjoueur" />
        <TextBox Grid.Column="1" Grid.Row="2"  Name="textBoxx" />
        <TextBox Grid.Column="2" Grid.Row="2" Name="textBoxy"/>
        <Button Content="Play" Grid.Column="1" Grid.Row="3" Name="buttonpl" Click="buttonpl_Click" />
        <TextBox Grid.Column="1"  Name="textBoxshowRes"  />
        <Button Content="LoadAssembly" Grid.Column="2" Grid.Row="3" Name="buttonLoadAssembly" Click="buttonLoadAssembly_Click" />
    </Grid>
</Window>
